{
  "name": "next-react",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -b",
    "clean": "rm -rf ./dist ./build tsconfig.tsbuildinfo",
    "dev": "npx styleguidist server",
    "copy": "cpx 'src/**/*.{scss,css,json,jpg,gif,png,svg,ttf,woff,woff2,eot,mp3,js}' dist",
    "test": "mkdir -p test-reports && npx testcafe -c 4 'chrome:headless --disable-setuid-sandbox --window-size=1920x1080' dist/components/base/**/_tests/index.intg.js --reporter xunit:test-reports/report.xml --app 'yarn serve'",
    "serve": "node testcafeStyleguidistServer.js",
    "testunit": "jest --silent --config ./baseConfig/jest.config-base.js --no-cache --ci",
    "test:unit": "jest --silent --config ./baseConfig/jest.config-base.js --no-cache",
    "testcafe": "npx testcafe chrome --compiler-options typescript.configPath=./tsconfig.testcafe.json",
    "prettier": "npx prettier --check \"src/**/*.{ts,tsx,js,jsx,scss}\"",
    "prettier:fix": "npx prettier --write \"src/**/*.{ts,tsx,js,jsx,scss}\"",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{ts,tsx}'",
    "fix": "yarn prettier:fix && yarn lint:fix",
    "validate": "yarn prettier && yarn lint",
    "generate:component": "./scripts/generate.sh component",
    "generate:hook": "./scripts/generate.sh hook"
  },
  "node": {
    "fs": "empty"
  },
  "files": [
    "src",
    "js"
  ],
  "dependencies": {
    "@paydock/client-sdk": "^1.1.18",
    "@sentry/browser": "^5.13.0",
    "@sentry/integrations": "^5.13.0",
    "@types/react-image-gallery": "^1.0.5",
    "@types/socket.io-client": "1.4.32",
    "@types/window-or-global": "^1.0.4",
    "axios": "^0.19.0",
    "calculate-size": "^1.1.1",
    "class-validator": "^0.9.1",
    "copy": "^0.3.2",
    "cpx": "^1.5.0",
    "d3": "^5.7.0",
    "d3-array": "^2.0.3",
    "d3-scale": "^3.0.0",
    "d3-selection": "^1.4.0",
    "d3-timer": "^1.0.9",
    "d3-transition": "^1.2.0",
    "date-fns": "^2.29.3",
    "dexie": "^3.0.0-alpha.8",
    "firebase": "^7.14.2",
    "http-proxy": "^1.18.1",
    "lottie-web": "~5.2.1",
    "mock-socket": "^8.0.5",
    "mustache": "^4.0.1",
    "next-shared": "https://github.com/neilanthonyte/next-shared.git",
    "notevil": "^1.3.3",
    "npm": "^6.4.1",
    "piexifjs": "^1.0.4",
    "query-string": "^6.9.0",
    "rc-time-picker": "^3.7.3",
    "react": "17.0.2",
    "react-canvas-draw": "^1.2.1",
    "react-code-input": "^3.8.1",
    "react-currency-format": "^1.0.0",
    "react-day-picker": "^8.3.7",
    "react-dropzone": "^10.1.5",
    "react-file-icon": "^1.0.0",
    "react-fluid-grid": "^1.0.9",
    "react-free-scrollbar": "^0.3.2",
    "react-image-gallery": "^0.8.16",
    "react-load-script": "^0.0.6",
    "react-lottie": "^1.2.3",
    "react-pdf": "^4.1.0",
    "react-popover": "^0.5.10",
    "react-qr-code": "^2.0.2",
    "react-query": "^2.5.13",
    "react-responsive-modal": "^6.4.1",
    "react-sizeme": "^2.6.7",
    "socket.io-client": "2.2.0",
    "soundmanager2": "^2.97.20170602",
    "three": "^0.88.0",
    "validate.js": "^0.13.1",
    "window-or-global": "^1.0.1"
  },
  "devDependencies": {
    "@types/d3": "^5.7.0",
    "@types/http-proxy": "^1.17.4",
    "@types/mustache": "^4.0.1",
    "@types/pluralize": "^0.0.29",
    "@types/react-canvas-draw": "^1.1.1",
    "@types/react-pdf": "^4.0.5",
    "@types/react-popover": "^0.5.3",
    "@types/soundmanager2": "^2.97.4",
    "@types/three": "^0.89.0",
    "react-styleguidist": "^12.0.0",
    "testcafe": "1.15.1"
  }
